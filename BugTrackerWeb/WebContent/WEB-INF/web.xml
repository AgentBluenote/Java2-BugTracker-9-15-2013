<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <description>BugTracker</description>
 <listener>
  <listener-class>edu.uci.java2.contoller.HibernateListener</listener-class>
 </listener>
 <servlet>
  <servlet-name>login</servlet-name>
  <display-name>BugTrackerLogin</display-name>
  <servlet-class>edu.uci.java2.contoller.LoginController</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>register</servlet-name>
  <display-name>RegisterUser</display-name>
  <servlet-class>edu.uci.java2.contoller.RegisterController</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>listofbugs</servlet-name>
  <display-name>ListOfBugs</display-name>
  <servlet-class>edu.uci.java2.contoller.ListOfBugController</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/login</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>register</servlet-name>
  <url-pattern>/register</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>listofbugs</servlet-name>
  <url-pattern>/listofbugs</url-pattern>
 </servlet-mapping>
</web-app>
